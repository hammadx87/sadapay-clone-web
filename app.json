{
  "expo": {
    "name": "SadaPay Clone",
    "slug": "sadapay-clone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "sadapay-clone",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sadapay.clone"
    },
    "android": {
      "package": "com.sadapay.clone",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FF7363"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/sadapay-logo.jpg",
          "resizeMode": "contain",
          "backgroundColor": "#FF7363"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0f752173-612e-45da-b7c2-f4a4d9e0a426"
      }
    }
  }
}
